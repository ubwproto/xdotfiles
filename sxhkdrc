alt + comma ; alt + p
  menux

super + w
  rofi -show combi -combi-modi "window,drun" -modi "window,drun,run,ssh,combi"

alt + comma ; w
  rofi -show drun -modi "window,drun"

alt + comma ; alt + w
  rofi -show window -combi-modi "window,drun" -modi "window,drun,run,ssh,combi"

alt + comma ; alt + f
  wmctrl -a xterm

alt + comma ; alt + l
  wmctrl -a firefox

alt + comma ; alt + e
  xdotool mousemove 10 10

alt + comma ; alt + {1-9}
  xdotool key --clearmodifiers --delay 200 "super+{1-9}"

